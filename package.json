{
  "name": "critter_tracker",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "start": "node server.js",
    "heroku-postbuild": "npm install --only=dev --no-shrinkwrap && npm run build"
  },
  "dependencies": {
    "@progress/kendo-data-query": "^1.5.3",
    "@progress/kendo-dropdowns-vue-wrapper": "^2020.1.423",
    "@progress/kendo-theme-default": "^4.17.0",
    "@progress/kendo-theme-material": "^3.14.0",
    "@progress/kendo-vue-animation": "^0.5.2",
    "@progress/kendo-vue-data-tools": "^0.5.2",
    "@progress/kendo-vue-grid": "^0.5.2",
    "@progress/kendo-vue-intl": "^0.5.2",
    "@progress/kendo-vue-popup": "^0.5.2",
    "@types/jquery": "^3.3.35",
    "ag-grid-community": "^23.0.2",
    "ag-grid-vue": "^23.0.2",
    "core-js": "^3.6.4",
    "darkmode-js": "^1.5.5",
    "express": "^4.17.1",
    "moment": "^2.24.0",
    "register-service-worker": "^1.7.1",
    "serve-static": "^1.14.1",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.3",
    "vue-mobile-detection": "^1.0.0",
    "vue-property-decorator": "^8.4.1",
    "vue-router": "^3.1.6"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "@vue/cli-plugin-babel": "~4.3.0",
    "@vue/cli-plugin-eslint": "~4.3.0",
    "@vue/cli-plugin-pwa": "~4.3.0",
    "@vue/cli-plugin-router": "~4.3.0",
    "@vue/cli-plugin-typescript": "~4.3.0",
    "@vue/cli-service": "~4.3.0",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "@vue/eslint-config-typescript": "^5.0.2",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-vue": "^6.2.2",
    "node-sass": "^7.0.0",
    "sass-loader": "^8.0.2",
    "typescript": "~3.8.3",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/airbnb",
      "@vue/typescript/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "rules": {
      "no-plusplus": 1,
      "eqeqeq": 1,
      "prefer-template": 1,
      "vue/no-unused-components": [
        "warn", 
        {
          "ignoreWhenBindingPresent": true
        }
      ],
      "no-param-reassign": [
        "error",
        {
          "props": false
        }
      ]
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
